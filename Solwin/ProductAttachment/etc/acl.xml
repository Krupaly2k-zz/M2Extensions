<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Solwin_ProductAttachment::extension" title="Solwin" sortOrder="61">
                    <resource id="Solwin_ProductAttachment::productattachment" title="Product Attachment" sortOrder="58">
